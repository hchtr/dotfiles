set nocompatible

syntax on
filetype on

set tabstop=2
set shiftwidth=2
set autoindent
set expandtab

set wildmenu

set number
set termguicolors
set background=dark
colo lunaperche 

inoremap ( ()<Esc>i
inoremap [ []<Esc>i
inoremap { {}<Esc>i
inoremap " ""<Esc>i
inoremap ' ''<Esc>i

set hlsearch
set incsearch
set ignorecase
